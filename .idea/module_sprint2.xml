<?xml version="1.0" encoding="UTF-8"?>
<project name="module_sprint2" default="compile.module.sprint2">
  <dirname property="module.sprint2.basedir" file="${ant.file.module_sprint2}"/>
  
  <property name="module.jdk.home.sprint2" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.sprint2" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.sprint2" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.sprint2" value="-encoding UTF-8 -source 9 -target 9 ${compiler.args}"/>
  
  <property name="sprint2.output.dir" value="undefined"/>
  <property name="sprint2.testoutput.dir" value="undefined"/>
  
  <path id="sprint2.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="sprint2.module.production.classpath"/>
  
  <path id="sprint2.runtime.production.module.classpath"/>
  
  <path id="sprint2.module.classpath"/>
  
  <path id="sprint2.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.sprint2">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.sprint2">
    <patternset refid="excluded.from.module.sprint2"/>
  </patternset>
  
  
  <target name="compile.module.sprint2" depends="compile.module.sprint2.production,compile.module.sprint2.tests" description="Compile module sprint2"/>
  
  <target name="compile.module.sprint2.production" depends="register.custom.compilers" description="Compile module sprint2; production classes"/>
  
  <target name="compile.module.sprint2.tests" depends="register.custom.compilers,compile.module.sprint2.production" description="compile module sprint2; test classes" unless="skip.tests"/>
  
  <target name="clean.module.sprint2" description="cleanup module">
    <delete dir="${sprint2.output.dir}"/>
    <delete dir="${sprint2.testoutput.dir}"/>
  </target>
</project>